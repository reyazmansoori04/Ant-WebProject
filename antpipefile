pipeline {
    agent any


    stages 
    {  
        
        stage ('git clone') {
        steps{
          git 'https://github.com/reyazmansoori04/Ant-WebProject'
         }
    
    }
    
       stage ('git build') {
        steps{
withAnt(installation: 'LocalAnt', jdk: 'LocalJDK'){
   
                    sh 'ant build'
                }
         }
    
    }
}
}
